<ETHERNETKRL>
  <CONFIGURATION>
        <EXTERNAL>
          <IP>x.x.x.x</IP>
          <PORT>7000</PORT>
          <TYPE>Server</TYPE>
        </EXTERNAL>
        <INTERNAL>
          <BUFFERING Mode="FIFO" Limit="32"/>
          <ALIVE Set_Flag="997"/>
        </INTERNAL>
      </CONFIGURATION>
  <SEND>
    <XML>
      <ELEMENT Tag="req/node/rc_silhouettematch" Type="STRING"/>
      <ELEMENT Tag="req/service/detect_object" Type="STRING"/>
      <ELEMENT Tag="req/args/pose_frame" Type="STRING"/>
      <ELEMENT Tag="req/args/offset" Type="REAL"/>
      <ELEMENT Tag="req/args/robot_pose/@X" Type="REAL"/>
      <ELEMENT Tag="req/args/robot_pose/@Y" Type="REAL"/>
      <ELEMENT Tag="req/args/robot_pose/@Z" Type="REAL"/>
      <ELEMENT Tag="req/args/robot_pose/@A" Type="REAL"/>
      <ELEMENT Tag="req/args/robot_pose/@B" Type="REAL"/>
      <ELEMENT Tag="req/args/robot_pose/@C" Type="REAL"/>
      <ELEMENT Tag="req/args/object_to_detect/@region_of_interest_2d_id" Type="STRING"/>
      <ELEMENT Tag="req/args/object_to_detect/@object_id" Type="STRING"/>
      <ELEMENT Tag="req/end_of_request" Type="BOOL"/>
    </XML>
  </SEND>
  <RECEIVE>
    <XML>
      <ELEMENT Tag="res/timestamp/@sec" Type="INT"/>
      <ELEMENT Tag="res/timestamp/@nsec" Type="INT"/>
      <ELEMENT Tag="res/return_code/@message" Type="STRING"/>
      <ELEMENT Tag="res/return_code/@value" Type="INT"/>
      <ELEMENT Tag="res/object_id" Type="STRING"/>
      <ELEMENT Tag="res/instances/le/timestamp/@sec" Type="INT"/>
      <ELEMENT Tag="res/instances/le/timestamp/@nsec" Type="INT"/>
      <ELEMENT Tag="res/instances/le/pose/@X" Type="REAL"/>
      <ELEMENT Tag="res/instances/le/pose/@Y" Type="REAL"/>
      <ELEMENT Tag="res/instances/le/pose/@Z" Type="REAL"/>
      <ELEMENT Tag="res/instances/le/pose/@A" Type="REAL"/>
      <ELEMENT Tag="res/instances/le/pose/@B" Type="REAL"/>
      <ELEMENT Tag="res/instances/le/pose/@C" Type="REAL"/>
      <ELEMENT Tag="res/instances/le/id" Type="STRING"/>
      <ELEMENT Tag="res/instances/le/object_id" Type="STRING"/>
      <ELEMENT Tag="res/instances/le/pose_frame" Type="STRING"/>
      <ELEMENT Tag="res" Set_Flag="998"/>
    </XML>
  </RECEIVE>
</ETHERNETKRL>
